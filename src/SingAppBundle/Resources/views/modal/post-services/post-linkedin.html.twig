{{ form_start(form, {'attr': {'class': 'add-new-post', 'enctype': 'multipart/form-data'}}) }}
                {{ form_errors(form) }}
<div class="modal-body">
    <div class="control-group col-md-12 col-sm-12">
        {{ form_row(form.title) }}
    </div>
    <div class="control-group col-md-12 col-sm-12">
        {{ form_row(form.caption) }}
    </div>
    <div class="control-group col-md-12 col-sm-12">
        {{ form_row(form.url) }}
    </div>
    <div class="control-group col-md-12 col-sm-12">
        {{ form_row(form.visibility) }}
    </div>
    {% include '@SingApp/modal/post-services/common-data.html.twig' with {'massage' : massage} %}
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
    <button type="submit" class="btn btn-primary">Create</button>
</div>
{{ form_end(form) }}